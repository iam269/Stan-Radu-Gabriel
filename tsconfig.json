{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./",
    "rootDir": "./",
    "noEmitOnError": true,
    "jsx": "react"
  },
  "include": ["script.tsx", "*.tsx"],
  "exclude": ["node_modules"]
}